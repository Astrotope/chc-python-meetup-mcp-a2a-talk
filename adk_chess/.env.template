# Chess MCP Server Configuration
MCP_SERVER_URL=http://localhost:8010/mcp

# Google AI Studio Authentication (required)
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_GENAI_USE_VERTEXAI=FALSE

# AgentOps (Observability Platform) Configuration
AGENTOPS_USE=false
AGENTOPS_API_KEY=your_agentops_api_key_here

# ADK Configuration
ADK_LOG_LEVEL=WARNING

# Agent Ports
ORCHESTRATOR_PORT=8007
WHITE_PLAYER_PORT=8005
BLACK_PLAYER_PORT=8006

# Agent URLs
ORCHESTRATOR_URL=http://localhost:8007
WHITE_PLAYER_URL=http://host.docker.internal:8005
BLACK_PLAYER_URL=http://host.docker.internal:8006
WHITE_PLAYER_HOST=host.docker.internal
BLACK_PLAYER_HOST=host.docker.internal

# UI Configuration
GRADIO_PORT=7865

# Agent Model Configuration
ORCHESTRATOR_MODEL=gemini-2.5-flash
WHITE_PLAYER_MODEL=gemini-2.5-flash
BLACK_PLAYER_MODEL=gemini-2.5-flash

# Planner Configuration
ORCHESTRATOR_THINKING_BUDGET=4096
PLAYER_THINKING_BUDGET=2048

# Automatic Function Calling Configuration
ORCHESTRATOR_MAX_CALLS=30
PLAYER_MAX_CALLS=10
